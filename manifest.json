{
  "name": "AutoJoomer",
  "version": "3.0.2",
  "manifest_version": 3,
  "icons": {
    "128": "src/images/icon.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://lms.iiitkottayam.ac.in/*", "https://lmsone.iiitkottayam.ac.in/*", "http://*/fgtauth*"
      ],
      "js": [
        "/src/js/logger.js"
      ],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_title": "AutoJoomer",
    "default_popup": "/src/js/popup.html"
  }
}
